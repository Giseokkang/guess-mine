  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babelify": "^10.0.0",
    "del": "^5.0.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-csso": "^3.0.1",
    "morgan": "^1.9.1",
    "pug": "^2.0.4",
    "socket.io": "^2.2.0",
    "nodemon": "^1.19.1",
    "gulp-browserify": "^0.5.1",
    "gulp-sass": "^4.0.2",
    "node-sass": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "babel-core": "^6.26.3",
    "core-js": "^2.6.9",
    "dotenv": "^8.0.0",
    "helmet": "^3.19.0",
    "lodash.template": ">=4.5.0",
    "shell-quote": ">=1.6.1"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/server --ignore assets/ --ignore src/static",
    "dev:assets": "gulp",
    "build:assets": "gulp",
    "build:server": "babel src --out-dir build --ignore 'src/static'",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run copyAll",
    "prebuild": "rm -rf build",
    "start": "node build/server",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "prettier": "1.18.2"
  }
}
